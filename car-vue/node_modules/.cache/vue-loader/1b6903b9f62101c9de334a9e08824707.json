}\n    },\n    mounted() {\n      this.fetchData;\n        },\n    method: {\n      fetchData: function() {\n          axios.get('https://carstockrest.herokuapp.com/cars')\n          .then(response => (this.cars = response.data._embedded.cars));\n      },\n      deleteCar: function(link) {\n          axios.delete(link)\n          .then(() => this.fetchData)\n      }\n\n    }\n  }\n</script>\n\n<style>\n  table {\n    margin-left:auto;\n    margin-right:auto;\n  }\n</style>\n"]}]}